def generate():
    
    
    file_loader = FileSystemLoader('.')

    env = Environment(loader=file_loader)

    template = env.get_template('CoreconfigTemplate.txt')

    #ip = input("Please enter ip address")

    output = template.render(hostname=e1.get(),project_name=e2.get(),revision_num=e3.get(),rr=e4.get(),loopback_ip=e5.get(),nms_main=e6.get(),nms_backup=e7.get(),mule_main=e8.get(),mule_backup=e9.get(),rsi=e10.get(),timezone_name=e11.get(),bdpriority_prim=e12.get(),bd10_ip=e13.get(),bd10_vip=e14.get(),bd20_ip=e15.get(),bd20_vip=e16.get(),bd150_ip=e17.get(),bd150_vip=e18.get(),bd151_ip=e19.get(),bd151_vip=e20.get(),bd161_ip=e21.get(),bd161_vip=e22.get(),bd200_ip=e23.get(),bd200_vip=e24.get(),bd400_ip=e25.get(),bd400_vip=e26.get(),bd410_ip=e27.get(),bd410_vip=e28.get(),bd7RR_ip=e29.get(),bd7RR_Vip=e30.get(),bd8RR_ip=e31.get(),bd8RR_vip=e32.get(),backbone_ip=e33.get(),snmp_loc=e34.get(),station_ip1=e35.get(),srt1_lo=e36.get(),core_ip2=e37.get(),crt2_lo=e38.get())

    fopen=open("Coreconfiguration11.txt","w")
    fopen.write(output)
    messagebox.showinfo("Configuration status","Successfully Generated")

    #fopen=open("jinja_output.txt","r")
    #readfile=fopen.read()
 
    fopen.close()
    
def clear():  
 
    e1.delete(0,50)
    e2.delete(0,50)
    e3.delete(0,50)
    e4.delete(0,50)
    e5.delete(0,50)
    e6.delete(0,50)
    e7.delete(0,50)
    e8.delete(0,50)
    e9.delete(0,50)
    e10.delete(0,50)
    e11.delete(0,50)
    e12.delete(0,50)
    e13.delete(0,50)
    e14.delete(0,50)
    e15.delete(0,50)
    e16.delete(0,50)
    e17.delete(0,50)
    e18.delete(0,50)
    e19.delete(0,50)
    e20.delete(0,50)
    e21.delete(0,50)
    e22.delete(0,50)
    e23.delete(0,50)
    e24.delete(0,50)
    e25.delete(0,50)
    e26.delete(0,50)
    e27.delete(0,50)
    e28.delete(0,50)
    e29.delete(0,50)
    e30.delete(0,50)
    e31.delete(0,50)
    e32.delete(0,50)
    e33.delete(0,50)
    e34.delete(0,50)
    e35.delete(0,50)
    e36.delete(0,50)
    e37.delete(0,50)
    e38.delete(0,50)
 
    
mybutton = tk.Button(root,text="Core router configuration",fg="blue",padx=7,pady=12,width=19,command = generate)
mybutton.grid(row=37,column=1)

mybutton1 = tk.Button(root,text="Clear",fg="blue",padx=7,pady=12,width=19,command = clear)
mybutton1.grid(row=37,column=2)


root.mainloop()
